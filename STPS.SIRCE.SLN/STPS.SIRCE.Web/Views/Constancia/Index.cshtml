@{
    ViewBag.Title = "Constancias";

}

<div id="contenido" class="col-lg-12">
    <div id="datosConstancia" titulo="Constancias: " icono="table" class="col-lg-12"></div>
</div>


<script type="text/javascript">

    //debugger;
    $(document).ready(function () {
        $($("#contenido").children("div")).dynamicPanels({});

        $.ajax({
            url: '@Url.Action("Accion")',
            type: 'GET',
            data: null,
            beforeSend: function () {
                $("body").dynamicSpinner({
                    loadingText: "Cargando Constancias..."
                });
            },
            success: function (result) {
                $("#datosConstancia .panel-body").html(result);
                $("body").dynamicSpinnerDestroy({});
            }
        });
    });
</script>
