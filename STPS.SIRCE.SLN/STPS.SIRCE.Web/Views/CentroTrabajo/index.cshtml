@model STPS.SIRCE.Web.Models.CentroTrabajoModel
@{
    ViewBag.Title = "Centro de Trabajo";
    Layout = null;
}

<div id="contenido" class="col-lg-12">
    <div id="filtrosEstablecimientos" titulo="Filtros de busqueda para Centros de Trabajo:" icono="filter" class="col-lg-12"></div>
</div>


<script type="text/javascript">
    $(document).ready(function () {
        //Modelo en Cache para llamadas posteriores a Servidor.
        var model = @Html.Raw(Json.Encode(Model));
        $($("#contenido").children("div")).dynamicPanels({});
        console.log(model);
        $.ajax({
            url: '@Url.Action("ConsultarVistaFiltros", "CentroTrabajo")',
            type: 'GET',
            data: {rawModel: JSON.stringify(model)},
            success: function (result) {
                $("#filtrosEstablecimientos .panel-body").html(result);
            }
        });

    });
</script>
